{
  "name": "cidao-bot",
  "version": "1.0.1",
  "description": "The Heleno son, created to have fun with Discord clients",
  "main": "src/main.js",
  "scripts": {
    "build": "tsc -p .",
    "start:prod": "node src/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "si": "nodemon --inspect -r tsconfig-paths/register ./node_modules/.bin/ts-node src/main.ts",
    "i": "node inspect -r tsconfig-paths/register ./node_modules/.bin/ts-node src/main.ts",
    "sib": "nodemon --inspect-brk -r tsconfig-paths/register ./node_modules/.bin/ts-node src/main.ts"
  },
  "keywords": [
    "discord",
    "bot"
  ],
  "author": "RafaelGSS",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "discord.js": "^12.2.0",
    "dotenv": "^8.2.0",
    "ffmpeg-static": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.4",
    "ts-node": "^8.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  }
}
